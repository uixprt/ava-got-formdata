{
  "name": "ava-integration",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "files": [
      "test/*"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
    ],
    "timeout": "1m"
  },
  "dependencies": {
    "ava": "^4.2.0",
    "dotenv": "^16.0.1",
    "formdata-node": "^4.3.2",
    "got": "^12.0.4"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/node": "^17.0.33",
    "prettier": "^2.6.2",
    "prettier-plugin-packagejson": "^2.2.18",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
